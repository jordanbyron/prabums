<h1>Editing note</h1>

<% form_for(@note) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :work_order_id %><br />
    <%= f.text_field :work_order_id %>
  </p>
  <p>
    <%= f.label :created_by_id %><br />
    <%= f.text_field :created_by_id %>
  </p>
  <p>
    <%= f.label :updated_by_id %><br />
    <%= f.text_field :updated_by_id %>
  </p>
  <p>
    <%= f.label :text %><br />
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @note %> |
<%= link_to 'Back', notes_path %>